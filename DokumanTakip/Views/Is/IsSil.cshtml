@model DokumanTakip.Model.Is

@{
    ViewBag.Title = "IsSil";
}

<h2></h2>

<h3></h3>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>İş</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">İş</a></li>
                        <li class="breadcrumb-item active">İş Sil</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Sil</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <div>
                            <h4></h4>
                            <hr />
                            <dl class="dl-horizontal">
                                <dt>
                                    @Html.DisplayNameFor(model => model.KayitTarihi)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.KayitTarihi)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.AktifMi)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.AktifMi)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.IsAdi)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.IsAdi)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.BildirimTarih)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.BildirimTarih)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.CozumTarihi)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.CozumTarihi)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.IsSahibi)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.IsSahibi)
                                </dd>

                            </dl>

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color">
                                    <input type="submit" value="Sil" class="btn btn-danger" /> |
                                    @Html.ActionLink("Listeye Git", "/NormalIsler")
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
